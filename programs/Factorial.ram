class Fac {
    public int ComputeFac(int num) {
		int num_aux ;
		if (num le 1)   // is num < 1?
			num_aux = 1 ;
		else 
			num_aux = num * (this.ComputeFac(num-1));
		return num_aux ;
    }
	
